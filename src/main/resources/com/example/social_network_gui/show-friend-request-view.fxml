<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.social_network_gui.controller.ShowFriendRequestController">
   <children>
      <Button layoutX="64.0" layoutY="333.0" mnemonicParsing="false" onMouseClicked="#accept_request" prefHeight="41.0" prefWidth="180.0" text="ACCEPT" textAlignment="CENTER" />
      <Button layoutX="354.0" layoutY="333.0" mnemonicParsing="false" onMouseClicked="#reject_request" prefHeight="41.0" prefWidth="180.0" text="REJECT" />
      <Label layoutX="222.0" layoutY="14.0" prefHeight="60.0" prefWidth="180.0" text="Your friend requests:" textFill="#4549b5">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewRequests" layoutX="41.0" layoutY="85.0" prefHeight="231.0" prefWidth="518.0">
        <columns>
          <TableColumn fx:id="tableColumnName" prefWidth="257.0" text="Name" />
          <TableColumn fx:id="tableColumnStatus" prefWidth="260.0" text="Status" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
